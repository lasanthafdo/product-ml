<!DOCTYPE html>
<meta http-equiv="content-type" content="text/html; charset=utf-8">
	<html>
	<head>
	 <link rel="stylesheet" type="text/css" href="../../css/mlmain.css">
	 <link rel="stylesheet" type="text/css" href="./css/importData.css">
	 <script type="text/javascript" src="../../js/d3.js"></script>
	 <script src="../../js/jquery.js"></script>
	 <script src="./js/importDataset.js"></script>
	</head>
	<body>
	<div id="middle">
	<%
	    include("../../includes/header.jag");
	    include("../../includes/tenantAware.jag");
	    session.put("datasetId",undefined);
	    session.put("projectId",undefined);
	%>

		<div id="workArea">
			<%include("../../includes/wizardmenu.jag");%>
			<script>disableWizardMenu();</script>
			<div id="importDialog">
				<form id="dataImportForm" action="./uploadDataset_ajax.jag" method="post"
					enctype="multipart/form-data">
					Project Name: <input type="text" name="projectName" id="projectName"/> 
					<br><br>
					Project Description: 
					<br><textarea rows="2" name="projectDescription" id="projectDescription"/></textarea>
					<br><br>
					Data Source: <input type="text" id="datasetName" name="datasetName" disabled/>
					<input type="file" size="60" name="uploadDataset" id="uploadDataset"> 
					<input type="button" id="datasetOpen" value="Browse" /> 
					<br><br>
					<input type="submit" value="Import">
					<span id="validatorMsg"></span>
				</form>
			</div>
	</div>
	</body>
	</html>
