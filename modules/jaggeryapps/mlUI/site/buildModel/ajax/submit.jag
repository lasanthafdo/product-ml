<%
	var log=new Log();
	var paramters = request.getAllParameters("UTF-8");

	var carbon = require('carbon');
	var datasetService = carbon.server.osgiService('org.wso2.carbon.ml.dataset.DatasetService');
	paramters.datasetURL = datasetService.getDataSource(session.get("datasetId"));
	paramters.columnSeparator = datasetService.getSeparator();

	log.info(paramters);
%>