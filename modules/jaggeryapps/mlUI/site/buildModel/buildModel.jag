    <script src="../../js/jquery.js"></script>
    <script src="../../js/jquery-ui.js"></script>
    <script src="./js/buildModel.js"></script> 

    <body>
        <div id="middle">
            <%
                include("../../includes/title.jag");
            %>
            <div id="workArea">
                <%include("../../includes/wizardmenu.jag");%>
                <div style="clear:both"></div>
                <div id="wizzardSteps">
                    <% include('algoType.jag'); %>
